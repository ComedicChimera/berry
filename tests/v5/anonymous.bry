import io.std;

func printAnon(x: struct { a: i64, b: i64 }) {
    std.PutInt(x.a);
    std.PutStr(", ");
    std.PutInt(x.b);
    std.PutRune('\n');
}

func createAnon(s: i64) struct { a: i64, b: i64 } {
    return .{a=s, b=-s};
}

func main() {
    let anon = createAnon();
    let anon2 = struct{a: i64, b: i64}{1, 1};

    printAnon(anon);
    printAnon(anon2);
}