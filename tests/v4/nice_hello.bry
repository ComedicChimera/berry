import io.std;

let nums: []i64 = [1, 2, 3, 4, 5];

func printArray(arr: []i64) {
    for let i = 0; i < arr._len; i++ {
        if i > 0 {
            std.PutStr(", ");
        }

        std.PutInt(arr[i]);
    }

    std.PutRune('\n');
}

func main() {
    std.PutStr("Hello, world!\n");
    printArray(nums);

    std.PutInt(abs(-20));
    std.PutRune('\n');
}