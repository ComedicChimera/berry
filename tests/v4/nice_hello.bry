import io.std;

let nums: []i64 = [1, 2, 3, 4, 5];

func print_array(arr: []i64) {
    for let i = 0; i < arr._len; i++ {
        if i > 0 {
            std.puts(", ");
        }

        std.putint(arr[i]);
    }

    std.putr('\n');
}

func main() {
    std.puts("Hello, world!\n");
    print_array(nums);

    std.putint(abs(-20));
    std.putr('\n');
}