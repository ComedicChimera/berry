func main() {
    if os.args.Len() != 2 {
        std.Errorln("error: usage: bf <filename>");
        os.Exit(1);
    }

    let file_name = os.args[1];

    if run(file_name) match .Err(e) {
        std.Errorf("error: %v\n", e);
        os.Exit(1)
